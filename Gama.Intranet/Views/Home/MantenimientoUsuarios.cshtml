    <!-- MODAL -->
    <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog modal-lg">
        <div class="modal-content">
          <div class="modal-header bg-primary">
            <h5 class="exampleModalLabel" id="exampleModalLabel">Administracion de <b>Usuario</b></h5>
            <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
          </div>
          <div class="modal-body">
            <form>
              <div class="row p-2">
                <div class="col">
                  <label class="form-label">id</label>
                  <input type="text" class="form-control" id="iduser" aria-label="name" disabled>
                </div>
                <div class="col">
                  <label class="form-label">Nombre</label>
                  <input type="text" class="form-control" id="name" placeholder="Ingrese el nombre" aria-label="name" required />
                </div>
              </div>
              <div class="row p-2">
                <div class="col">
                  <label class="form-label">Contraseña</label>
                  <input type="password" class="form-control" id="password" placeholder="Ingrese la contraseña" required
                    aria-label="name" />
                </div>

                <div class="col">
                  <label class="form-label">Status</label>
                  <div class="form-group">
                    <select class="form-control" id="exampleFormControlSelect1 status">
                      <option>Activo</option>
                      <option>Desahabilitado</option>
                    </select>
                  </div>
                </div>
              </div>
                 <div class="row p-2">
                <div class="col">
                  <label class="form-label">Role</label>
                  <div class="form-group">
                    <select class="form-control" id="exampleFormControlSelect1 role" required>
                      <option>Administrador</option>
                      <option>Usuario</option>
                      <option>Cliente</option>
                      <option>Vendedor</option>
                      <option>Cajero</option>
                    </select>
                  </div>
                </div>
                <div class="d-grid gap-2 d-md-block">
                  <button class="btn btn-warning" type="button">Restablecer Contraseña</button>
                </div>
              </div>
            </form>
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Cancelar</button>
            <button type="submit" class="btn btn-success" onclick="validar_campos()">Guardar</button>
          </div>
        </div>
      </div>
    </div>

